<ChartContainer>
    <ApexChart TItem="SupportIncident">
        <ApexBoxPlotSeries TItem="SupportIncident"
                    Items="incidents.OrderBy(e=>e.WeekNumber)"
                    Name="incidents"
                    XValue="@(e => e.WeekName)"
                    YValue="@(e => e.Severity)" />
    </ApexChart>
</ChartContainer>

@code {
    private List<SupportIncident> incidents { get; set; } = SampleData.GetSupportIncidents(30,60);
    }